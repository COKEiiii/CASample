    <groupId>sg.nus.iss</groupId>
    <artifactId>ShoppingCart</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>ShoppingCart</name>
    <description>ShoppingCart</description>
    <url/>
    <licenses>
        <license/>
